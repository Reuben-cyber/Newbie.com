<%-include('headerad'); -%>

<html>
<head>
  
	<link rel="stylesheet" type="text/css" href="./style1.css"> 
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<style>
body {margin: 0;
  /* border-color: white; */
}
.addform input[type=text], select {
  width: 50%;
  padding: 12px 20px;
  margin: 8px 0;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 90px;
  box-sizing: border-box;
}

.addform input[type=submit] {
  width: 50%;
  background-color: #4CAF50;
  color: white;
  padding: 14px 20px;
  margin: 8px 0;
  border: none;
  border-radius: 4px;
  cursor: pointer;
}

.addform input[type=submit]:hover {
  background-color: #45a049;
}

.addform {
  border-bottom-right-radius:20px;
  background-color:#f4f0ec;
}
#deleteuser {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

#deleteuser td, #deleteuser th {
  border: 1px solid #ddd;
  padding: 8px;
}

#deleteuser tr:nth-child(even){background-color: #f2f2f2;}

#deleteuser tr:hover {background-color: #ddd;}

#deleteuser th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #090a09;
  color: white;  
}
</style>

	</head>
<body>

 
 <ul class="topnav">
  <li><a class="active" href="/adminfunction">Add User</a></li>
  <li><a href="/deleteusersad">List of Users</a></li>
  <li><a href="/delproductad">List of Product</a></li>
  <li><a href="/deleducationad">List of Education</a></li>
  <li><a href="/delfoodad">List of Food</a></li>
  <li><a href="/payments-list">List of Payments</a></li>
  <li><a href="/delplacesad">List of Places</a></li>
  <li><a href="/delshoppingad">List of Shopping</a></li>
  <li><a href="/delstayad">List of Stay</a></li>
  <li><a href="/deltravelad">List of Travel</a></li>
			
</ul>


   <div class="table-data">
   <center>
<h2>List of Places</h2>
    <table id="deleteuser">
        <tr>
          <th>S.N</th>
	<th>Place_id</th>
            <th>Name</th>
            <th>Description</th>
            <th>Photos</th>
            <th>Prices</th>
            <th>Reviews</th>
			<th>Location</th>
			<th>Timing</th>
			<th>Contact</th>
			<th>Usr_id</th>
			
			<th>Delete</th>
        </tr>
        
        <%
        if(placesData.length!=0){
        var i=1;
        placesData.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.pl_id %></td>
            <td><%=data.name %></td>
            <td><%=data.description %></td>
            <td><%=data.photos %></td>
            <td><%=data.prices %></td>
			<td><%=data.reviews %></td>
			<td><%=data.location %></td>
			<td><%=data.timings %></td>
			<td><%=data.contact %></td>
			<td><%=data.usr_id %></td>
			
            
            <td><a href="/delplacesad/delete/<%=data.p_id%>">Delete</a></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
	</center>
    </div>
</body>
</html>


    